    "import numpy as np\n",
    "import cv2\n",
    "\n",
    "image = np.full((255,255), 0)\n",
    "\n",
    "count = 0\n",
    "for j in range(255):\n",
    "    for i in range(255):\n",
    "        image[i][j] = count\n",
    "        \n",
    "    if(j%50 == 0):\n",
    "        count += j\n",
    "\n",
    "cv2.imwrite(\"img3.png\", image)"
   ]
  }
